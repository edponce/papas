{
    "hello": {
        "program": "apps/hello.py",
        "params": {
            "xparam": [10, 30],
            "yparam": 20
        },
        "cmd": "${program}"
    },

    "hello2": {
        "program": "${hello:program}",
        "params": {
            "infile": "text.in",
            "$": "hello:params",
            "outfile": "text.out"
        },
        "cmd": "${program}",
        "before": "hello"
    }
}
